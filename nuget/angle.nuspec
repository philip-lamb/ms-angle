<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>ANGLE.WindowsStore.Servo</id>
        <version>2.1.20</version>
        <title>Servo's fork of ANGLE for Windows Store (UWP)</title>
        <authors>Microsoft &amp; the Servo team</authors>
        <owners>jdm</owners>
        <description>Runs OpenGL ES content on Windows by translating OpenGL ES API calls to DirectX 11 API calls.
Targets Universal Windows 10 apps (UWP). See 'ANGLE.WindowsStore' for non-ARM64 apps.</description>
        <projectUrl>http://github.com/microsoft/angle/wiki</projectUrl>
        <license type="file">LICENSE</license>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <copyright>Copyright (C) 2002-2013 The ANGLE Project Authors. Portions Copyright (C) Microsoft Corporation.</copyright>
        <tags>OpenGL ES GLES GLSL Direct3D graphics 3D</tags>
    </metadata>

    <files>
      <!-- UAP -->
      <file src="..\out\$configuration$-x86\libEGL.dll" target="bin\UAP\x86" />
      <file src="..\out\$configuration$-x86\libEGL.dll.pdb" target="bin\UAP\x86" />
      <file src="..\out\$configuration$-x86\libGLESv2.dll" target="bin\UAP\x86" />
      <file src="..\out\$configuration$-x86\libGLESv2.dll.pdb" target="bin\UAP\x86" />
      <file src="..\out\$configuration$-x86\libEGL.dll.lib" target="bin\UAP\x86\libEGL.lib" />
      <file src="..\out\$configuration$-x86\libGLESv2.dll.lib" target="bin\UAP\x86\libGLESv2.lib" />

      <file src="..\out\$configuration$-x64\libEGL.dll" target="bin\UAP\x64" />
      <file src="..\out\$configuration$-x64\libEGL.dll.pdb" target="bin\UAP\x64" />
      <file src="..\out\$configuration$-x64\libGLESv2.dll" target="bin\UAP\x64" />
      <file src="..\out\$configuration$-x64\libGLESv2.dll.pdb" target="bin\UAP\x64" />
      <file src="..\out\$configuration$-x64\libEGL.dll.lib" target="bin\UAP\x64\libEGL.lib" />
      <file src="..\out\$configuration$-x64\libGLESv2.dll.lib" target="bin\UAP\x64\libGLESv2.lib" />

      <file src="..\out\$configuration$-arm64\libEGL.dll" target="bin\UAP\ARM64" />
      <file src="..\out\$configuration$-arm64\libEGL.dll.pdb" target="bin\UAP\ARM64" />
      <file src="..\out\$configuration$-arm64\libGLESv2.dll" target="bin\UAP\ARM64" />
      <file src="..\out\$configuration$-arm64\libGLESv2.dll.pdb" target="bin\UAP\ARM64" />
      <file src="..\out\$configuration$-arm64\libEGL.dll.lib" target="bin\UAP\ARM64\libEGL.lib" />
      <file src="..\out\$configuration$-arm64\libGLESv2.dll.lib" target="bin\UAP\ARM64\libGLESv2.lib" />

      <!-- Headers -->
      <file src="..\include\**\*.h" target="include" />
      <file src="..\include\**\*.inc" target="include" />

      <!-- Source files -->
      <file src="..\src\**\*.cpp" target="src" />

      <!-- Build scripts -->
      <file src="ANGLE.WindowsStore.Servo.targets" target="build\native" />

      <!-- Other files -->
      <file src="..\AUTHORS" />
      <file src="..\CONTRIBUTORS" />
      <file src="..\LICENSE" />
    </files>
</package>